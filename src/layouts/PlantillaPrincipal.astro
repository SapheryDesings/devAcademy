---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import "../styles/global.css"
interface Props { titulopagina?: string; }
const { titulopagina } = Astro.props;
---
<html lang="es">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Titan+One&display=swap" rel="stylesheet">
		<meta name="generator" content={Astro.generator} />
		<title>devAcademy | { titulopagina }</title>
	</head>
	<body>
		<Header/>
			<section class="contenidoPrincipal">
				<slot/>
			</section>
		<Footer/>
	</body>
</html>